
{
  "family": "my-ecs-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "*",
  "containerDefinitions": [
    {
      "name": "nginx-container",
      "image": "nginx:latest",
      "portMappings": [{ "containerPort": 80, "protocol": "tcp" }],
      "essential": true
    }
  ]
}
